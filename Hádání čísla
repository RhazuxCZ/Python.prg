import random
secret_number = random.randint(1, 100)
guess = None
while guess != secret_number:
    guess = int(input("Hádej číslo mezi 1 a 100: "))
    if guess < secret_number:
        print("Zkus vyšší číslo.")
    elif guess > secret_number:
        print("Zkus nižší číslo.")
    else:
        print("Uhodl jsi!")
